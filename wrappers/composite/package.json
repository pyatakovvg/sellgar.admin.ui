{
  "name": "@wrapper/composite",
  "version": "0.0.1",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types",
  "scripts": {
    "build:types": "tsc",
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./lib --extensions '.ts,.tsx,.js,.jsx' --copy-files --no-copy-ignored",
    "start:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files --watch"
  },
  "dependencies": {
    "@wrapper/default": "0.0.1",
    "react": "18.2.0",
    "react-redux": "8.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "cross-env": "7.0.3"
  }
}
