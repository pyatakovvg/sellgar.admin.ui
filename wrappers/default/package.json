    "react": "18.1.0",
    "react-redux": "8.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "cross-env": "7.0.3"
  }
}
